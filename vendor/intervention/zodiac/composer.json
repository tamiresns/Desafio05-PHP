{
    "name": "intervention/zodiac",
    "description": "Zodiac sign calculator",
    "keywords": ["zodiac", "sun sign", "star sign", "horoscope", "astrology", "birthday"],
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Vogel",
            "email": "oliver@olivervogel.com",
            "homepage": "https://olivervogel.com/"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "nesbot/carbon": "^2.0",
        "illuminate/translation": "~4.0|~5.0|~6.0|~7.0|~8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^0.12.11"
    },
    "autoload": {
        "psr-4": {
            "Intervention\\Zodiac\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Intervention\\Zodiac\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Intervention\\Zodiac\\ZodiacServiceProvider"
            ],
            "aliases": {
                "Zodiac": "Intervention\\Zodiac\\Facades\\Zodiac"
